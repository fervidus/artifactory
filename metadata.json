{
  "name": "landcareresearch-artifactory",
  "version": "3.0.6",
  "author": "Fervid",
  "summary": "Installs and configures Artifactory OSS",
  "license": "Apache-2.0",
  "source": "https://github.com/manaakiwhenua/artifactory.git",
  "project_page": "https://github.com/manaakiwhenua/artifactory",
